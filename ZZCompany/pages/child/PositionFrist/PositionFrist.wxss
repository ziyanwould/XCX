/* pages/child/PositionFrist/PositionFrist.wxss */
@import "/template/template.wxss";  
.bubai{
  background: #FFFFFF;
}
page{
  background: #EFEFEF;
}
.popup{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  position: fixed;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  
}
.popupCount{
 width: 628rpx;
 height: 395rpx;
 background: #FFFFFF;
 border-radius:25rpx;
 position: relative;
 padding: 20rpx;
}
.popupCount image{
  width: 102rpx;
  height: 102rpx;
  border-radius: 102rpx;
}
.popupCount .close{
  position: absolute;
  font-size: 50rpx;
  right: 0;
  top: 0;
  padding: 0 20rpx 20rpx 20rpx;
}
.popupCount .title{
  display: inline-flex;
   align-items:center;
  padding-top: 40rpx;
}
.popupCount .title text{
 font-size: 31rpx;
 font-weight: 600;
 color: #000000;
 padding-left: 36rpx;
}
.kj,.kj2{
  display: inline-block;
  width: 40rpx;
  height: 30rpx;
  padding-right: 28rpx;
}
.kj{
  background: url('http://www.liujiarong.top/WXImg/HDphone.png') no-repeat center center;
  background-size: 17rpx 24rpx;
}
.kj2{
  background: url('http://www.liujiarong.top/WXImg/HDemail.png') no-repeat center center;
  background-size: 23rpx 17rpx;
}
.child{
  padding-top: 40rpx;
   display: inline-flex;
   align-items:center;
}
.child text:nth-of-type(2){
  color: #aaaaaa;
}
 .bannerImg{
  width: 100%;
  height: 160rpx;
  overflow: hidden;
 background: #00B38B;
} 